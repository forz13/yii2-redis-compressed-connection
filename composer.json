{
  "name": "forz13/yii2-redis-compressed-connection",
  "description": "Redis gzip data compression extension",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension"
  ],
  "license": "BSD-3-Clause",
  "version": "0.2.10",
  "authors": [
    {
      "name": "Kalashnikov Sergey",
      "email": "kalashnikov.sergey13@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "^2.0.12",
    "yiisoft/yii2-redis": "~2.0.0",
    "ext-zlib": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "forz13\\redisCompressedConnection\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  }
}
